    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^3.4.0",
    "mocha": "^3.3.0",
    "mocha-css": "^1.0.1",
    "null-loader": "^0.1.1",
    "postcss": "^6.0.0",
    "postcss-cssnext": "^2.10.0",
    "postcss-less": "^3.0.2",
    "pre-commit": "^1.2.0",
    "rimraf": "^2.6.0",
    "sinon": "2.2.0",
    "sinon-chai": "^2.10.0",
    "style-loader": "^0.17.0",
    "stylefmt": "^5.3.0",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-processor-html": "^1.0.0",
    "svg-inline-loader": "^0.8.0",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^1.1.1",
    "vue": "^2.3.0",
    "vue-loader": "^12.0.0",
    "vue-template-compiler": "^2.3.0",
    "webpack": "^2.7.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^3.1.11",
    "webpack-merge": "^4.0.0",
    "yarn-or-npm": "^2.0.0"
  },
  "peerDependencies": {
    "axios": "^0.18.0",
    "vue-i18n": "^8.1.0"
  },
  "dllPlugin": {
    "name": "vuePluginTemplateDeps",
    "include": [
      "mocha/mocha.js",
      "style-loader!css-loader!mocha-css",
      "html-entities",
      "vue/dist/vue.js",
      "chai",
      "core-js/library",
      "url",
      "sockjs-client",
      "vue-style-loader/lib/addStylesClient.js",
      "events",
      "ansi-html",
